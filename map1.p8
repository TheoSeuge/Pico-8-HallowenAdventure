pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
--jeu d'aventure

function _init()
	create_player()
end

function _update()
	player_mouvement()
end

function _draw()
	cls()
	draw_map()
	draw_player()
end
-->8
--map

function draw_map()
	map(0,0,0,0,128,64)
end

function check_flag(flag,x,y)
	local sprite=mget(x,y)
	return fget(sprite,flag)
end
-->8
--player

function create_player()
	p={x=55,y=55,sprite=16}
end

function draw_player()
	spr(p.sprite,p.x*8,p.y*8)
end

function player_mouvement()
	newx=p.x
	newy=p.y
	if (btnp(➡️)) newx+=1
	if (btnp(⬅️)) newx-=1
	if (btnp(⬆️)) newy-=1
	if (btnp(⬇️)) newy+=1
	
	if not  check_flag(0,newx,newy) then
		p.x=newx
		p.y=newy
		--il y a un obstacle
		if (p.x<0) p.x=0
		if (p.y<0) p.y=0
		if (p.x>127) p.x=127
		if (p.y>63) p.y=63
	end
end
